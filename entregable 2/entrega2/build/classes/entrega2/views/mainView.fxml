              <MenuItem fx:id="abrir" mnemonicParsing="false" onAction="#abrirGPX" text="Abrir Archivo GPX" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Acerca de...">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#acercaDe" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <VBox fx:id="vbox" />
   </children>
